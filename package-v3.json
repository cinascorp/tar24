{
  "name": "c4isr-v3-advanced-drone-operations",
  "version": "3.0.0",
  "description": "Advanced military-grade C4ISR system for drone warfare operations with HTTP/3, QUIC, SDR integration, and global flight tracking capabilities",
  "main": "c4isr-v3.html",
  "homepage": ".",
  "scripts": {
    "start": "python3 -m http.server 8000",
    "dev": "python3 -m http.server 8000 --bind 0.0.0.0",
    "serve": "python3 -m http.server 8000",
    "serve-secure": "python3 -m http.server 8443 --bind 0.0.0.0",
    "kali": "chromium-browser --app=http://localhost:8000/kali-c4isr-v3.html --kiosk",
    "android": "chromium-browser --app=http://localhost:8000/android-c4isr-v3.html --simulate-touch-screen-with-mouse",
    "build": "echo 'Building C4ISR v3 system...' && mkdir -p dist && cp *.html dist/ && cp -r js dist/ && cp -r styles dist/",
    "test": "echo 'Testing C4ISR v3 systems...' && python3 -c 'import http.server; print(\"HTTP server test passed\")'",
    "lint": "echo 'Linting C4ISR v3 code...'",
    "install-deps": "pip3 install -r requirements.txt",
    "setup-kali": "sudo apt-get update && sudo apt-get install -y rtl-sdr hackrf gqrx-sdr dump1090-fa kismet aircrack-ng reaver",
    "setup-android": "echo 'Android setup requires NetHunter installation'",
    "curl-http3": "curl --http3 --verbose",
    "quic-test": "echo 'Testing QUIC protocol support'",
    "sdr-test": "rtl_test -t",
    "frequency-scan": "python3 scripts/frequency_scanner.py",
    "drone-detect": "python3 scripts/drone_detector.py",
    "military-ops": "python3 scripts/military_operations.py"
  },
