  "installation": {
    "kali_linux": [
      "git clone https://github.com/military-ops/c4isr-v3-advanced-drone-operations.git",
      "cd c4isr-v3-advanced-drone-operations",
      "chmod +x install-kali.sh",
      "./install-kali.sh",
      "npm run setup-kali",
